{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.cs" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../../bindings/csharp"
        }
      ],
      "dest": "../../build/docfx/obj/api",
      "force": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "../../build/docfx/obj/api",
        "dest": "api"
      },
      {
        "files": [ "**/*.md", "*.md", "toc.yml" ],
        "src": "../../docs"
      }
    ],
    "resource": [
      {
        "files": [ "images/**"],
        "src": "../../docs"
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "docfx Microsoft Cognitive Services Speech SDK website",
      "_enableSearch": true
    },
    "dest": "../../build/docfx/_site",
    "xrefService_uncomment": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
