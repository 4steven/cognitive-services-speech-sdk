<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <SpeechPackageRoot>$(MSBuildThisFileDirectory)..\..\</SpeechPackageRoot>
  </PropertyGroup>
  <ItemGroup>
    <None Include="$(SpeechPackageRoot)build\native\$(Platform)\Release\carbon.dll">
      <Link>carbon.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Include="$(SpeechPackageRoot)build\native\$(Platform)\Release\carbon_csharp_bindings.dll">
      <Link>carbon_csharp_bindings.dll</Link>
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
